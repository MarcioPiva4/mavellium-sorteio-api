generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sorteio {
  id        Int  @id @default(autoincrement())
  nome      String
  whatsapp  String
  interesse String
  createdAt DateTime @default(now())
  company   String?
  email     String
  objetivo  String
  role      String?
}
